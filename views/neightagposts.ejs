<% if (postData.length === 0) { %>
<h2>No posts yet!</h2>
<button type="button" class="btn btn-link"><a href='/user/addpost'>Add a Post</a></button>
<% } else { %>
<% for (var i = 0; i < postData.length; i++) { %>
	<li><%= postData[i].title %> -
	<%= postData[i].content %> </li>
</div>
<% } %>
<% } %>

